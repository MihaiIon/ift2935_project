<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="10.0" maxWidth="Infinity" vgap="5.0"
          xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="sample.controllers.seller.SellerSummaryController">
    <stylesheets>
        <URL value="@../../styles.css" />
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="10.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="NEVER" />
        <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
    </rowConstraints>
    <children>
        <Label text="Summary" GridPane.rowIndex="0" />
        <ListView fx:id="listView" maxHeight="Infinity" maxWidth="Infinity" styleClass="o-list" GridPane.rowIndex="1" />
    </children>
</GridPane>
