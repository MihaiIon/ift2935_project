<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane hgap="10.0" maxWidth="Infinity" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../../styles.css"/>
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="10.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" vgrow="ALWAYS"/>
    </rowConstraints>
    <children>
        <Label text="Summary" GridPane.rowIndex="0"/>
        <ListView styleClass="o-list" maxHeight="Infinity" maxWidth="Infinity" GridPane.rowIndex="1"/>
    </children>
</GridPane>
